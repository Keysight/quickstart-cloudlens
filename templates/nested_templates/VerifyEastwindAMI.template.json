{ 
    "AWSTemplateFormatVersion": "2010-09-09", 
    "Description": "Verify if eastwind subscription is active by checking output of this stack. Copyright 2018  Keysight Technologies",
    "Parameters":{
        "ToolAMIInfoFunctionArn":{
            "Description": "Eastwind ami sensor id",
            "Type": "String"
        }
    },
    "Resources": {
        "ToolAMIInfo": {
            "Type": "Custom::AMIInfo",
            "Properties": {
              "ServiceToken": { "Ref": "ToolAMIInfoFunctionArn" },
              "Region": { "Ref": "AWS::Region" }
            }
        }
    },         
    "Outputs" : { 
        "isEastwindSubscriptionActive" : {
            "Value" : {
                 "Fn::GetAtt": [ "ToolAMIInfo", "isAMI" ]
             }
        }
    } 
}